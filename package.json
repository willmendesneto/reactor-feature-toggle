{
  "name": "reactor-feature-toggle",
  "version": "1.1.0",
  "main": "dist/main.js",
  "scripts": {
    "postinstall": "flow-typed update && flow-typed install",
    "build": "cross-env NODE_ENV=production webpack",
    "prepublish": "yarn run build",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf coverage",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha ./tests/unit/**/*.spec.js --require ./tests/unit/setup.js",
    "editorconfig-tools-check": "editorconfig-tools check ./src/** ./tests/**",
    "flow:stop": "flow stop",
    "flow-typed:update": "flow-typed update",
    "flow-typed:install": "flow-typed install",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js' -t html -t text",
    "flow:coverage:open": "yarn run flow:coverage && open ./flow-coverage/index.html"
  },
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "keywords": [
    "react",
    "reactjs",
    "feature-toggle",
    "feature-flag",
    "reactor-feature-toggle"
  ],
  "author": "Will Mendes <willmendesneto@gmail.com> (http://github.com/willmendesneto)",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "cross-env": "^5.0.5",
    "editorconfig-tools": "^0.1.1",
    "enzyme": "^2.8.2",
    "eslint": "^4.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-react": "^7.0.1",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^1.0.0",
    "flow-bin": "^0.56.0",
    "flow-coverage-report": "^0.3.0",
    "jsdom": "^11.0.0",
    "mocha": "^4.0.0",
    "nyc": "^11.0.2",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^16.0.0",
    "sinon": "^3.2.1",
    "url-loader": "^0.6.1",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "feature-toggle-service": "^2.0.1",
    "react": "^15.0.2"
  }
}
